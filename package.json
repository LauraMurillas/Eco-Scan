{
  "name": "reconocimiento-basura-gemini",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "vite": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "server": "node server.js",
    "start": "concurrently \"npm run server\" \"npm run dev\" --names \"BACKEND,FRONTEND\" --prefix-colors \"bgBlue.bold,bgMagenta.bold\"",
    "vercel-build": "vite build",
    "test:backend": "vitest tests/backend.integration.test.js --environment=node --run",
    "test:frontend": "vitest tests/frontend.integration.test.jsx --environment=jsdom --run",
    "test:integration": "npx vitest run tests/backend.integration.test.js tests/App.test.jsx tests/App.classify.test.jsx tests/QuizModal.test.jsx"  
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "groq-sdk": "^0.36.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.6.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^9.2.1",
    "jsdom": "^22.1.0",
    "msw": "^2.12.3",
    "nock": "^14.0.10",
    "supertest": "^6.3.3",
    "vite": "^5.0.12",
    "vitest": "^1.3.5"
  },
  "vitest": {
    "globals": true,
    "environment": "jsdom",
    "setupFiles": [
      "tests/setupTests.js"
    ]
  }
}
